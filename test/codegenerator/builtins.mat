mat a = [[1,2,3],
         [4,5,6],
         [5,6,7],
         [8,9,10]]

print_descriptor(a)

mat b = copy(a)

print_descriptor(b)

print_descriptor(shape(a))

printdouble(dimensions(a))

print_descriptor(reshape(a,[6,2]))

mat c[3,3]

print_descriptor(fill(c, 3.14))

print_descriptor(arange(c))

print_descriptor(eye(c))

print_descriptor(range(0,10,1))

printdouble(length(range(0,10,1)))

print_descriptor(range(10,0,1))

print_descriptor(range(10,0,-1))

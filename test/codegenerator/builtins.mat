extern fun mat copy(mat m)
extern fun mat shape(mat m)
extern fun mat reshape(mat m, mat shape)
extern fun double dimensions(mat m)

extern fun void print_descriptor(mat m)
extern fun void printdouble(double d)

mat a = [[1,2,3],
         [4,5,6],
         [5,6,7],
         [8,9,10]]

print_descriptor(a)

mat b = copy(a)

print_descriptor(b)

print_descriptor(shape(a))

printdouble(dimensions(a))

print_descriptor(reshape(a,[6,2]))

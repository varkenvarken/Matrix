extern fun void printdouble(double d)
extern fun void printstring(str s)

str hello = "Hello"

fun void myfunc(str a, double b):
    double bbb = 3.14 + b     // we should that var refs are to initialized vars
    str aaa = a + " world"    // assignment does not check type yet
    printdouble(bbb)
    printstring(aaa)

myfunc(hello, -3.14)
